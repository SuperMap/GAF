<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.supermap.gaf.data.mgt.mapper.MmTableMapper">
    <resultMap id="BaseResultMap" type="com.supermap.gaf.data.mgt.entity.MmTable">
        <result column="table_id" jdbcType="VARCHAR" property="tableId" />
        <result column="table_name" jdbcType="VARCHAR" property="tableName" />
        <result column="model_id" jdbcType="VARCHAR" property="modelId" />
        <result column="sdx_info" jdbcType="VARCHAR" property="sdxInfo" />
        <result column="sort_sn" jdbcType="SMALLINT" property="sortSn" />
        <result column="description" jdbcType="VARCHAR" property="description" />
        <result column="created_time" jdbcType="TIMESTAMP" property="createdTime" />
        <result column="created_by" jdbcType="VARCHAR" property="createdBy" />
        <result column="updated_time" jdbcType="TIMESTAMP" property="updatedTime" />
        <result column="updated_by" jdbcType="VARCHAR" property="updatedBy" />
    </resultMap>

    <sql id="Base_Column_List">
        table_id,
        table_name,
        model_id,
        sdx_info,
        sort_sn,
        description,
        created_time,
        created_by,
        updated_time,
        updated_by
    </sql>
    <select id="select" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_List" />
        FROM "mm_table"
        WHERE table_id = #{tableId}
    </select>
    <select id="selectList" resultMap="BaseResultMap" >
        SELECT <include refid="Base_Column_List" />
        FROM "mm_table"
        WHERE
        <trim>
            <if test='tableId != null'> table_id = #{tableId} AND</if>
            <if test='tableName != null'> table_name = #{tableName} AND</if>
            <if test='modelId != null'> model_id = #{modelId} AND</if>
            <if test='sdxInfo != null'> sdx_info = #{sdxInfo} AND </if>
            <if test='sortSn != null'> sort_sn = #{sortSn} AND </if>
            <if test='description != null'>  description = #{description} AND</if>
            <if test='createdTime != null'>  created_time = #{createdTime} AND</if>
            <if test='createdBy != null'>  created_by = #{createdBy} AND</if>
            <if test='updatedTime != null'>  updated_time = #{updatedTime} AND</if>
            <if test='updatedBy != null'>  updated_by = #{updatedBy} AND</if>
            <if test ='null != equalFieldName'>  ${equalFieldName}
                <choose>
                    <when test="equalFieldValue != null"> = #{equalFieldValue} </when>
                    <otherwise>  is null </otherwise>
                </choose>
                AND
            </if>
            <if test ='null != searchFieldName and null != searchFieldValue'>  ${searchFieldName} LIKE CONCAT('%', #{searchFieldValue}, '%')  AND</if>
            1=1
        </trim>
        <if test ='null != orderFieldName'>order by ${orderFieldName} <if test ='null != orderMethod'> ${orderMethod}</if></if>
    </select>
    <insert id="insert" useGeneratedKeys="true" keyColumn="table_id" keyProperty="tableId" parameterType="com.supermap.gaf.data.mgt.entity.MmTable">
        INSERT INTO "mm_table"
        <trim prefix="(" suffix=")" suffixOverrides=",">
			<if test ='null != tableId'>table_id,</if>
			<if test ='null != tableName'>table_name,</if>
			<if test ='null != modelId'>model_id,</if>
			<if test ='null != sdxInfo'>sdx_info,</if>
			<if test ='null != sortSn'>sort_sn,</if>
			<if test ='null != description'>description,</if>
			created_time,
			<if test ='null != createdBy'>created_by,</if>
			updated_time,
			<if test ='null != updatedBy'>updated_by,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test ='null != tableId'>#{tableId},</if>
			<if test ='null != tableName'>#{tableName},</if>
			<if test ='null != modelId'>#{modelId},</if>
			<if test ='null != sdxInfo'>#{sdxInfo},</if>
			<if test ='null != sortSn'>#{sortSn},</if>
			<if test ='null != description'>#{description},</if>
			now(),
			<if test ='null != createdBy'>#{createdBy},</if>
			now(),
			<if test ='null != updatedBy'>#{updatedBy},</if>
        </trim>
    </insert>
	<insert id="batchInsert" parameterType="java.util.List">
        INSERT INTO "mm_table"(<include refid="Base_Column_List" />) VALUES
        <foreach collection="list" item="item" index="index" separator=",">
            (  #{item.tableId}  ,  #{item.tableName}  ,  #{item.modelId}  ,  #{item.sdxInfo}  ,  #{item.sortSn}  ,  #{item.description}  ,  now()  ,  #{item.createdBy}  ,  now()  ,  #{item.updatedBy}  )
        </foreach>
    </insert>
	<update id="batchDelete" parameterType="java.util.List">
        DELETE FROM mm_table
        WHERE table_id IN
        <foreach collection="list" index="index" item="id" open="(" close=")" separator=",">
            #{id}
        </foreach>
    </update>
	<update id="delete" >
        DELETE FROM mm_table
        WHERE table_id = #{tableId}
    </update>
    <update id="update" parameterType="com.supermap.gaf.data.mgt.entity.MmTable">
        UPDATE "mm_table"
        <set>
			<if test ="null != tableName and '' != tableName">table_name = #{tableName},</if>
			<if test ="null != modelId and '' != modelId">model_id = #{modelId},</if>				
			sdx_info = #{sdxInfo},
			sort_sn = #{sortSn},
			description = #{description},
			updated_time = now(),
			updated_by = #{updatedBy},
        </set>
        WHERE table_id = #{tableId}
    </update>

</mapper>