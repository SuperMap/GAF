    <select id="selectList" resultMap="BaseResultMap" >
        SELECT <include refid="Base_Column_List" />
        FROM "mm_field"
        WHERE
        <trim>
            <if test='fieldId != null'>  field_id = #{fieldId} AND </if>
            <if test='tableId != null'>  table_id = #{tableId} AND </if>
            <if test='fieldName != null'>  field_name = #{fieldName} AND </if>
            <if test='fieldAlias != null'>  field_alias = #{fieldAlias} AND </if>
            <if test='fieldType != null'>  field_type = #{fieldType} AND </if>
