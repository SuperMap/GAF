            #{id}
        </foreach>
    </update>
	<update id="delete" >
        DELETE FROM mm_field
        WHERE field_id = #{fieldId}
    </update>
    <update id="update" parameterType="com.supermap.gaf.data.mgt.entity.MmField">
        UPDATE "mm_field"
        <set>
			<if test ="null != tableId and '' != tableId">table_id = #{tableId},</if>
			<if test ="null != fieldName and '' != fieldName">field_name = #{fieldName},</if>				
			field_alias = #{fieldAlias},
			<if test ="null != fieldType and '' != fieldType">field_type = #{fieldType},</if>				
			field_length = #{fieldLength},
            field_precision = #{fieldPrecision},
			field_default = #{fieldDefault},
			<if test ="null != fieldNotNull and '' != fieldNotNull">field_not_null = #{fieldNotNull},</if>				
			<if test ="null != fieldPrimaryKey and '' != fieldPrimaryKey">field_primary_key = #{fieldPrimaryKey},</if>				
			description = #{description},
			updated_time = now(),
			updated_by = #{updatedBy},
            <if test ="null != sortSn and '' != sortSn">sort_sn = #{sortSn},</if>
        </set>
        WHERE field_id = #{fieldId}
