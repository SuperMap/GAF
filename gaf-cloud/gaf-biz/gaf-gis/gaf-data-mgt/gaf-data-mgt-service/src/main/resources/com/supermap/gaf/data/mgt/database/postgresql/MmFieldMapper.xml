			<if test ='null != createdBy'>#{createdBy},</if>
			now(),
			<if test ='null != updatedBy'>#{updatedBy},</if>
			<if test ='null != sortSn'>#{sortSn},</if>
        </trim>
    </insert>
	<insert id="batchInsert" parameterType="java.util.List">
        INSERT INTO "mm_field"(<include refid="Base_Column_List" />) VALUES
        <foreach collection="list" item="item" index="index" separator=",">
            (  #{item.fieldId}  ,  #{item.tableId}  ,  #{item.fieldName}  ,  #{item.fieldAlias}  ,  #{item.fieldType}  ,  #{item.fieldLength}  , #{item.fieldPrecision},  #{item.fieldDefault}  ,  #{item.fieldNotNull}  ,  #{item.fieldPrimaryKey}  ,  #{item.description}  ,  now()  ,  #{item.createdBy}  ,  now()  ,  #{item.updatedBy} ,#{item.sortSn} )
