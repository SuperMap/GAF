			<if test ='null != tableId'>#{tableId},</if>
			<if test ='null != fieldName'>#{fieldName},</if>
			<if test ='null != fieldAlias'>#{fieldAlias},</if>
			<if test ='null != fieldType'>#{fieldType},</if>
			<if test ='null != fieldLength'>#{fieldLength},</if>
			<if test ='null != fieldPrecision'>#{fieldPrecision},</if>
			<if test ='null != fieldDefault'>#{fieldDefault},</if>
